{
	"author": "Graham Dyson",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "HTML page that renders Eunice YAML as SVG, so the structure and dependencies can be investigated.",
	"dependencies": {
		"@devsnicket/eunice-dependency-and-structure": "4.6.0",
		"@devsnicket/eunice-renderer": "7.12.0",
		"core-js": "3.6.4",
		"fs-extra": "7.0.1",
		"react": "16.4.0",
		"react-dom": "16.4.2",
		"react-reflex": "2.2.8",
		"string-pixel-width": "1.7.0"
	},
	"devDependencies": {
		"@babel/core": "7.8.4",
		"@babel/plugin-transform-modules-commonjs": "7.8.3",
		"@babel/polyfill": "7.8.3",
		"@babel/preset-env": "7.8.4",
		"@babel/register": "7.8.3",
		"@devsnicket/eunice-test-harnesses": "8.15.0",
		"@types/fs-extra": "7.0.0",
		"@types/jest": "25.1.3",
		"@types/node": "12.12.28",
		"@types/react": "16.4.18",
		"@types/react-dom": "16.9.5",
		"@types/string-pixel-width": "1.7.0",
		"@types/webpack": "4.41.6",
		"babel-loader": "8.0.6",
		"cspell": "3.1.3",
		"eslint": "6.8.0",
		"eslint-plugin-notice": "0.8.9",
		"jest": "25.1.0",
		"typescript": "3.8.2",
		"webpack-cli": "3.3.2"
	},
	"homepage": "https://github.com/DevSnicket/eunice-renderer-test-harness#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!createWebpackConfiguration.js",
			"!createYamlInputElement.js",
			"!createYamlOutputElement/index.js",
			"!dist/**",
			"!harness.js",
			"!node_modules/**",
			"!test-coverage/**",
			"!tests/**",
			"!webpack.config.js",
			"!writeHarness/tests/output/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		},
		"moduleNameMapper": {
			"\\.css$": "<rootDir>tests/styleMock.js"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!@devsnicket)"
		]
	},
	"keywords": [
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"javascript",
		"render",
		"stack",
		"structure",
		"svg",
		"uince",
		"unice",
		"yaml"
	],
	"license": "SEE LICENSE IN LICENSE",
	"main": "index.js",
	"name": "@devsnicket/eunice-renderer-test-harness",
	"publishConfig": {
		"repository": "https://pkgs.dev.azure.com/grahamdyson/_packaging/Eunice/npm/registry/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-renderer-test-harness.git"
	},
	"sideEffects": [
		"*.css"
	],
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run typescript && npm run build && npm run test && npm run harness-example-production",
		"build": "rm -rf dist && npx webpack --config webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs -p",
		"eunice": "npx eunice --accept-license --ignore-path-pattern=\"(^|/)(\\..*|dist|gh-pages|node_modules|output|test-cases|test-coverage)$\" --output-yaml --reverse-file-content=false",
		"harness-dogfooding-development": "rm -rf tests/dogfoodingFromEuniceRepositoryRelativeDirectory/output && npx webpack --config tests/dogfoodingFromEuniceRepositoryRelativeDirectory/webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs -d",
		"harness-dogfooding-production": "rm -rf tests/dogfoodingFromEuniceRepositoryRelativeDirectory/output && npx webpack --config tests/dogfoodingFromEuniceRepositoryRelativeDirectory/webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs -p",
		"harness-example-development": "rm -rf tests/example/output && npx webpack --config tests/example/webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs -d",
		"harness-example-production": "rm -rf tests/example/output && npx webpack --config tests/example/webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs -p",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./dist/*\" ! -path \"./node_modules/*\" ! -path \"./tests/dogfoodingFromEuniceRepositoryRelativeDirectory/output/*\" ! -path \"./tests/example/output/*\" ! -path \"./test-coverage/*\" ! -path \"./writeHarness/tests/output/*\" -exec npx cspell {} +",
		"test": "npx jest",
		"typescript": "npx tsc -p jsconfig.json",
		"watch": "npx webpack --config webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs -d --watch"
	},
	"version": "5.0.1"
}
