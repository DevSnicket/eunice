{
	"author": "Graham Dyson",
	"description": "Test harness with two columns, YAML with processors for dogfooding the whole of Eunice and SVG renderer.",
	"dependencies": {
		"@devsnicket/eunice-dependency-and-structure": "2.8.0",
		"@devsnicket/eunice-processors": "3.0.0",
		"@devsnicket/eunice-renderer": "4.5.2",
		"@devsnicket/eunice-test-harnesses": "7.0.0",
		"@devsnicket/eunice-test-harnesses-processor-plugins": "1.0.0",
		"@devsnicket/plugin-discovery-create-repository": "1.5.1",
		"@ungap/url-search-params": "0.1.2",
		"react": "16.4.0",
		"react-dom": "16.4.2",
		"react-reflex": "2.2.8",
		"string-pixel-width": "1.7.0"
	},
	"devDependencies": {
		"@babel/polyfill": "7.4.4",
		"@babel/preset-env": "7.4.4",
		"@devsnicket/plugin-discovery-commonjs-babel-plugin": "1.10.1",
		"@types/jest": "24.0.11",
		"@types/node": "10.14.5",
		"@types/react": "16.4.18",
		"babel-loader": "8.0.6",
		"cspell": "3.1.3",
		"eslint": "5.9.0",
		"jest": "24.1.0",
		"webpack-cli": "3.3.2"
	},
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"createYamlOutputElement/createElementsFromYaml/**/*.js"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		},
		"moduleNameMapper": {
			"\\.css$": "<rootDir>tests/styleMock.js"
		}
	},
	"license": "MIT",
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run test && npm run build",
		"build": "rm -rf ../output/renderer && npx webpack --config webpack.config.js -p",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./output/**\" ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell --config ../../cspell.json {} +",
		"test": "npx jest",
		"watch": "npx webpack --config webpack.config.js -d --watch"
	}
}
