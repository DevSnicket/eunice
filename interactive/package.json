{
	"author": "Graham Dyson",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "HTML page that renders Eunice YAML as SVG, so the structure and dependencies can be investigated.",
	"dependencies": {
		"@devsnicket/eunice-dependency-and-structure": "^2.18.0",
		"@devsnicket/eunice-processors": "^10.4.0",
		"@devsnicket/eunice-renderer": "^6.7.0",
		"@devsnicket/eunice-test-harnesses": "^8.5.0",
		"fs-extra": "7.0.1",
		"react": "16.4.0",
		"react-dom": "16.4.2",
		"react-reflex": "2.2.8",
		"string-pixel-width": "1.7.0"
	},
	"devDependencies": {
		"@babel/polyfill": "7.4.4",
		"@babel/preset-env": "7.4.4",
		"@types/jest": "24.0.19",
		"@types/node": "10.14.5",
		"@types/react": "16.4.18",
		"babel-loader": "8.0.6",
		"cspell": "3.1.3",
		"eslint": "6.2.2",
		"eslint-plugin-notice": "0.8.9",
		"jest": "24.9.0",
		"webpack-cli": "3.3.2"
	},
	"homepage": "https://github.com/DevSnicket/eunice-renderer-test-harness#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!createWebpackConfiguration.js",
			"!createYamlInputElement/**",
			"!createYamlOutputElement/getProcessedYamlFromState.js",
			"!createYamlOutputElement/index.js",
			"!dist/**",
			"!harness.js",
			"!node_modules/**",
			"!test-coverage/**",
			"!tests/**",
			"!webpack.config.js",
			"!writeHarness/test/output/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		},
		"moduleNameMapper": {
			"\\.css$": "<rootDir>tests/styleMock.js"
		}
	},
	"keywords": [
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"javascript",
		"render",
		"stack",
		"structure",
		"svg",
		"uince",
		"unice",
		"yaml"
	],
	"license": "SEE LICENSE IN LICENSE",
	"main": "index.js",
	"name": "@devsnicket/eunice-renderer-test-harness",
	"publishConfig": {
		"repository": "https://pkgs.dev.azure.com/grahamdyson/_packaging/Eunice/npm/registry/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-renderer-test-harness.git"
	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run build && npm run test && npm run harness-example-production",
		"build": "rm -rf dist && npx webpack --config webpack.config.js -p",
		"harness-dogfooding-development": "rm -rf tests/dogfoodingFromEuniceRepositoryRelativeDirectory/output && npx webpack --config tests/dogfoodingFromEuniceRepositoryRelativeDirectory/webpack.config.js -d",
		"harness-dogfooding-production": "rm -rf tests/dogfoodingFromEuniceRepositoryRelativeDirectory/output && npx webpack --config tests/dogfoodingFromEuniceRepositoryRelativeDirectory/webpack.config.js -p",
		"harness-example-development": "rm -rf tests/example/output && npx webpack --config tests/example/webpack.config.js -d",
		"harness-example-production": "rm -rf tests/example/output && npx webpack --config tests/example/webpack.config.js -p",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./dist/*\" ! -path \"./node_modules/*\" ! -path \"./tests/dogfoodingFromEuniceRepositoryRelativeDirectory/output/*\" ! -path \"./tests/example/output/*\" ! -path \"./test-coverage/*\" ! -path \"./writeHarness/test/output/*\" -exec npx cspell {} +",
		"test": "npx jest",
		"watch": "npx webpack --config webpack.config.js -d --watch"
	},
	"version": "4.13.0"
}
