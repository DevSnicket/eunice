{
	"author": "Graham Dyson",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"dependencies": {
		"@devsnicket/eunice-dependency-and-structure": "^4.10.0"
	},
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"@babel/plugin-transform-modules-commonjs": "^7.10.4",
		"@devsnicket/eunice-run-tests-from-file-system": "^4.6.1",
		"@types/fs-extra": "^9.0.1",
		"@types/jest": "^26.0.7",
		"@types/node": "^10.17.28",
		"cspell": "^4.0.63",
		"eslint": "^7.5.0",
		"eslint-plugin-notice": "^0.9.10",
		"jest": "^26.1.0",
		"typescript": "^3.9.7"
	},
	"homepage": "https://devsnicket.com/eunice",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		},
		"transformIgnorePatterns": [
			"node_modules/(?!@devsnicket)"
		]
	},
	"keywords": [
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"processors",
		"stack",
		"structure",
		"uince",
		"unice",
		"yaml"
	],
	"license": "MIT",
	"name": "@devsnicket/eunice-stacking-inference",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-stacking-inference.git"
	},
	"sideEffects": false,
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run typescript && npm run test",
		"eunice": "npx eunice --accept-license --ignore-path-pattern=\"(^|/)(\\..*|node_modules|test-coverage)$\" --output-yaml --reverse-file-content=false",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +",
		"test": "npx jest",
		"typescript": "npx tsc -p jsconfig.json"
	},
	"version": "3.0.0"
}
