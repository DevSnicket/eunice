{
	"author": "Graham Dyson",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "visualizes a stack with categorized dependency counts as SVG",
	"dependencies": {
		"core-js": "^3.6.5",
		"string-pixel-width": "^1.10.0"
	},
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"@babel/node": "^7.10.5",
		"@babel/plugin-transform-modules-commonjs": "^7.10.4",
		"@devsnicket/eunice-dependency-and-structure": "^6.0.0",
		"@devsnicket/eunice-dependency-counter": "^3.0.0",
		"@devsnicket/eunice-run-tests-from-file-system": "^4.6.1",
		"@types/fs-extra": "^9.0.1",
		"@types/jest": "^26.0.7",
		"@types/lodash.deburr": "^4.1.6",
		"@types/node": "^14.0.26",
		"@types/react": "^16.9.43",
		"@types/react-dom": "^16.9.8",
		"@types/string-pixel-width": "^1.7.1",
		"cspell": "^4.0.63",
		"eslint": "^7.5.0",
		"eslint-plugin-notice": "^0.9.10",
		"fs-extra": "^9.0.1",
		"jest": "^26.1.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"typescript": "^3.9.7",
		"xml-formatter": "^2.1.3"
	},
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		},
		"transformIgnorePatterns": [
			"node_modules/(?!@devsnicket)"
		]
	},
	"homepage": "https://devsnicket.com/eunice",
	"license": "SEE LICENSE IN LICENSE",
	"main": "index.js",
	"name": "@devsnicket/eunice-visualizer",
	"publishConfig": {
		"repository": "https://pkgs.dev.azure.com/grahamdyson/_packaging/Eunice/npm/registry/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-visualizer.git"
	},
	"sideEffects": false,
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run typescript && npm run test && npm run github-pages-copy",
		"eunice": "npx -p eunice eunice --accept-license --directories=. --ignore-path-pattern=\\\"(^|/)(\\..*|gh-pages|node_modules|test-cases|test-coverage)$\\\" --output-yaml  --root-item-identifiers=visualizer --reverse-file-content=false",
		"lint": "npx eslint .",
		"github-pages-copy": "rm -f -rf gh-pages && mkdir gh-pages && bash github-pages-copy.sh",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +",
		"test": "npx jest",
		"typescript": "npx tsc -p jsconfig.json"
	},
	"version": "2.1.0"
}
