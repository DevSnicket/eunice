{
	"configurations":
		[
			{
				"localRoot": "${workspaceFolder}",
				"name": "attach",
				"remoteRoot": "${command:extension.vscode-wsl-workspaceFolder}",
				"request": "attach",
				"type": "node"
			},
			{
				"args": [ "--javaScript=" ],
				"cwd": "${workspaceFolder}/",
				"localRoot": "${workspaceFolder}",
				"name": "analyzer getYamlFromJavaScript",
				"program": "${workspaceFolder}/getYamlFromJavaScript/index.js",
				"remoteRoot": "${command:extension.vscode-wsl-workspaceFolder}",
				"request": "launch",
				"type": "node",
				"useWSL": true
			},
			{
				"args": [ ],
				"cwd": "${workspaceFolder}/",
				"localRoot": "${workspaceFolder}",
				"name": "analyzer getYamlFromJavaScript test",
				"program": "${workspaceFolder}/getYamlFromJavaScript/test.js",
				"remoteRoot": "${command:extension.vscode-wsl-workspaceFolder}",
				"request": "launch",
				"type": "node",
				"useWSL": true
			},
			{
				"args":
					[ 
						"--runInBand",
						"getYamlFromJavaScript/test.js"
					],
				"cwd": "${workspaceFolder}/",
				"localRoot": "${workspaceFolder}",
				"name": "analyzer getYamlFromJavaScript jest",
				"program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
				"remoteRoot": "${command:extension.vscode-wsl-workspaceFolder}",
				"request": "launch",
				"type": "node",
				"useWSL": true
			},
			{
				"args":
					[
						"--directory=./",
						"--ignoreDirectoryNames=dist",
						"--ignoreDirectoryNames=node_modules",
						"--ignoreDirectoryNames=output",
						"--ignoreDirectoryNames=test-cases",
						"--ignoreDirectoryNames=test-coverage"
					],
				"cwd": "${workspaceFolder}",
				"localRoot": "${workspaceFolder}",
				"name": "analyzer getOrCreateItemsInDirectory",
				"program": "${workspaceFolder}/getOrCreateItemsInDirectory/index.js",
				"remoteRoot": "${command:extension.vscode-wsl-workspaceFolder}",
				"request": "launch",
				"type": "node",
				"useWSL": true
			},
			{
				"args": 
					[ 
						"--runInBand",
						"getOrCreateItemsInDirectory/test.js"
					],
				"cwd": "${workspaceFolder}/",
				"localRoot": "${workspaceFolder}",
				"name": "analyzer getOrCreateItemsInDirectory jest",
				"program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
				"remoteRoot": "${command:extension.vscode-wsl-workspaceFolder}",
				"request": "launch",
				"type": "node",
				"useWSL": true
			},
		],
}