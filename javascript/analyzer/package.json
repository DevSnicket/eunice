{
	"author": "Graham Dyson",
	"bin": {
	  "eunice-javascript-analyzer-getOrCreateItemsInDirectory": "bin/getOrCreateItemsInDirectory.js",
	  "eunice-javascript-analyzer-getYamlFromJavaScript": "bin/getYamlFromJavaScript.js"
	},
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Analyzes JavaScript files/directories and writes a YAML file of the dependencies which can then be processed, rendered and interacted with using the rest of Eunice.",
	"dependencies": {
		"@devsnicket/eunice-call-when-process-entry-point": "1.2.1",
		"acorn": "5.6.2",
		"array.prototype.flatmap": "1.2.1",
		"babel-polyfill": "6.26.0",
		"js-yaml": "3.11.0"
	},
	"devDependencies": {
		"@devsnicket/eunice-run-tests-from-file-system": "1.1.0",
		"@devsnicket/eunice-test-harnesses": "2.1.6",
		"@types/acorn": "4.0.3",
		"@types/jest": "23.3.1",
		"babel-cli": "6.26.0",
		"babel-loader": "7.1.4",
		"babel-preset-env": "1.7.0",
		"eslint": "4.19.1",
		"jest": "23.3.0",
		"react": "16.4.0",
		"react-dom": "16.4.0",
		"react-reflex": "2.2.8",
		"webpack": "4.11.1",
		"webpack-cli": "3.0.3"
	},
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!**/test-cases/**",
			"!**/test-cases/**/.js",
			"!dist/**",
			"!node_modules/**",
			"!harness/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"keywords": [
		"analyze",
		"analyzer",
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"javascript",
		"stack",
		"structure",
		"uince",
		"unice",
		"yaml"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"name": "@devsnicket/eunice-javascript-analyzer",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-javascript-analyzer.git"
	},
	"scripts": {
		"build": "babel -d dist --ignore dist/*,dist/**/*,harness/*,node_modules/**/*,**/test/**,**/testcases/**,test-coverage/**,**/test.js --plugins transform-object-rest-spread -s true .",
		"dogfooding": "node getOrCreateItemsInDirectory --directory=. --ignoreDirectoryNames=dist --ignoreDirectoryNames=node_modules --ignoreDirectoryNames=output --ignoreDirectoryNames=test-cases --ignoreDirectoryNames=test-coverage",
		"harness": "webpack -p --config harness/webpack.config.js",
		"lint": "eslint .",
		"test": "jest --runInBand"
	},
	"version": "1.8.2"
}
