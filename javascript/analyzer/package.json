{
	"author": "Graham Dyson",
	"bin": {
		"eunice-javascript-analyzer-getOrCreateItemsInDirectory": "dist/getOrCreateItemsInDirectory/bin.js",
		"eunice-javascript-analyzer-getYamlFromJavaScript": "dist/getYamlFromJavaScript/bin.js"
	},
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Analyzes JavaScript files/directories and writes a YAML file of the dependencies which can then be processed, rendered and interacted with using the rest of Eunice.",
	"dependencies": {
		"@devsnicket/eunice-call-with-process-standard-streams": "1.0.0",
		"acorn": "5.6.2",
		"array.prototype.flatmap": "1.2.1",
		"js-yaml": "3.13.1"
	},
	"devDependencies": {
		"@babel/cli": "7.4.4",
		"@babel/core": "7.4.4",
		"@babel/polyfill": "7.4.4",
		"@babel/preset-env": "7.4.4",
		"@devsnicket/eunice-run-tests-from-file-system": "1.5.1",
		"@devsnicket/eunice-test-harnesses": "7.2.0",
		"@types/acorn": "4.0.3",
		"@types/jest": "24.0.13",
		"@types/node": "10.14.7",
		"babel-loader": "8.0.6",
		"cspell": "4.0.21",
		"eslint": "5.16.0",
		"eslint-plugin-notice": "0.8.8",
		"jest": "24.8.0",
		"react": "16.4.0",
		"react-dom": "16.4.2",
		"react-reflex": "2.2.8",
		"webpack-cli": "3.3.2"
	},
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!**/bin.js",
			"!**/test-cases/**",
			"!**/test-cases/**/.js",
			"!dist/**",
			"!node_modules/**",
			"!harness/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"keywords": [
		"analyze",
		"analyzer",
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"javascript",
		"stack",
		"structure",
		"uince",
		"unice",
		"yaml"
	],
	"license": "GPL-3.0-or-later",
	"main": "dist/index.js",
	"name": "@devsnicket/eunice-javascript-analyzer",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-javascript-analyzer.git"
	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run build && npm run test && npm run harness && npm run dogfooding",
		"build": "rm -rf dist && babel -d dist --ignore dist/*,dist/**/*,harness/output/**,node_modules/**/*,**/test/**,**/test-cases/**,test-coverage/**,**/test.js --plugins @babel/plugin-proposal-object-rest-spread -s true .",
		"dogfooding": "node getOrCreateItemsInDirectory/bin.js --directory=. --ignoreDirectoryNames=dist --ignoreDirectoryNames=node_modules --ignoreDirectoryNames=output --ignoreDirectoryNames=test-cases --ignoreDirectoryNames=test-coverage",
		"harness": "rm -rf harness/output && npx webpack -p --config harness/webpack.config.js",
		"lint": "eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./dist/*\" ! -path \"./dist/**/*\" ! -path \"./harness/output/*\" ! -path \"./node_modules/**/*\" ! -path \"**/test-cases/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +",
		"test": "jest --runInBand"
	},
	"version": "3.10.0"
}
