{
	"author": "Graham Dyson",
	"bin": {
		"eunice-javascript-analyzer-getOrCreateItemsInDirectory": "getOrCreateItemsInDirectory/bin.js",
		"eunice-javascript-analyzer-getYamlFromJavascript": "getYamlFromJavascript/bin.js"
	},
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Analyzes JavaScript files/directories and writes a YAML file of the dependencies which can then be processed, rendered and interacted with using the rest of Eunice.",
	"dependencies": {
		"@babel/parser": "7.5.5",
		"@babel/types": "7.5.5",
		"@devsnicket/eunice-call-with-process-standard-streams": "^1.1.0",
		"array.prototype.flat": "1.2.3",
		"array.prototype.flatmap": "1.2.1",
		"js-yaml": "3.13.1"
	},
	"devDependencies": {
		"@babel/polyfill": "7.4.4",
		"@babel/preset-env": "7.4.4",
		"@devsnicket/eunice-run-tests-from-file-system": "4.0.0",
		"@devsnicket/eunice-test-harnesses": "7.10.0",
		"@types/jest": "24.0.19",
		"@types/node": "10.14.7",
		"babel-loader": "8.0.6",
		"cspell": "4.0.21",
		"eslint": "6.2.2",
		"eslint-plugin-notice": "0.8.9",
		"fs-extra": "8.1.0",
		"jest": "24.9.0",
		"react": "16.4.0",
		"react-dom": "16.4.2",
		"react-reflex": "2.2.8",
		"webpack-cli": "3.3.2"
	},
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!**/bin.js",
			"!**/test-cases/**",
			"!**/test-cases/**/.js",
			"!node_modules/**",
			"!harness/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"keywords": [
		"analyze",
		"analyzer",
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"javascript",
		"stack",
		"structure",
		"uince",
		"unice",
		"yaml"
	],
	"license": "SEE LICENSE IN LICENSE",
	"main": "index.js",
	"name": "@devsnicket/eunice-javascript-analyzer",
	"publishConfig": {
		"repository": "https://pkgs.dev.azure.com/grahamdyson/_packaging/Eunice/npm/registry/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-javascript-analyzer.git"
	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run test && npm run harness-production && npm run dogfooding",
		"dogfooding": "node getOrCreateItemsInDirectory/bin.js --directory=. --ignorePathPattern=\"(^(.git|node_modules|harness/output|test-coverage))|/test-cases\"",
		"eunice": "npx eunice --accept-license --ignore-path-pattern=\"(^|/)(\\..*|gh-pages|node_modules|output|test-cases|test-coverage)$\" --infer-stacks --modify-stacks-file=modify-file-stacks.yaml --modify-stacks-key=type --modify-stacks-pattern=^file$ --output-yaml --reverse-file-content=false",
		"harness-production": "rm -rf harness/output && npx webpack -p --config harness/webpack.config.js",
		"harness-development": "rm -rf harness/output && npx webpack -d --config harness/webpack.config.js",
		"lint": "eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./harness/output/*\" ! -path \"./node_modules/**/*\" ! -path \"**/test-cases/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +",
		"test": "jest --runInBand"
	},
	"version": "11.20.0"
}
