{
	"author": "Graham Dyson",
	"bin": {
		"eunice": "bin/index.js"
	},
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Analyzes JavaScript on the file system, processes the analysis and renders the processed analysis to an SVG.",
	"dependencies": {
		"@devsnicket/eunice-javascript-analyzer": "^2.0.1",
		"@devsnicket/eunice-processors": "^3.2.0",
		"@devsnicket/eunice-renderer": "*",
		"@devsnicket/eunice-renderer-test-harness": "^1.2.0",
		"js-yaml": "3.13.1",
		"minimist": "1.2.0"
	},
	"devDependencies": {
		"@devsnicket/eunice-call-with-process-standard-streams": "*",
		"@devsnicket/eunice-dependency-and-structure": "*",
		"@devsnicket/eunice-run-tests-from-file-system": "*",
		"@devsnicket/eunice-test-harnesses": "*",
		"@devsnicket/eunice-test-harnesses-processor-plugins": "*",
		"@types/node": "12.0.2",
		"cspell": "4.0.7",
		"eslint": "5.16.0"
	},
	"homepage": "https://github.com/DevSnicket/eunice-javascript#readme",
	"keywords": [
		"analyze",
		"analyzer",
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"javascript",
		"render",
		"stack",
		"structure",
		"svg",
		"uince",
		"unice",
		"yaml"
	],
	"license": "MIT",
	"main": "index.js",
	"name": "@devsnicket/eunice",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-javascript.git"
	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run dogfooding",
		"dogfooding": "npm update && node bin --directoryToCreateOrAddToStacksFrom=dogfooding --outputDirectoryPath=dogfooding --packageNames=call-with-process-standard-streams --packageNames=dependency-and-structure --packageNames=javascript-analyzer --packageNames=processors --packageNames=renderer --packageNames=renderer-test-harness --packageNames=run-tests-from-file-system --packageNames=test-harnesses --packageNames=test-harnesses-processor-plugins --packagePrefix=@devsnicket/eunice- --directories=. --identifierPrefixesOfRootItems=@devsnicket/eunice-javascript",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./output/**\" ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +"
	},
	"version": "2.3.0"
}
