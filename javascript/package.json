{
	"author": "Graham Dyson",
	"bin": {
		"eunice": "bin/index.js"
	},
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Analyzes JavaScript on the file system, processes the analysis and renders the processed analysis to an SVG.",
	"dependencies": {
		"@devsnicket/eunice-javascript-analyzer": "^2.0.2",
		"@devsnicket/eunice-processors": "^3.5.0",
		"@devsnicket/eunice-renderer": "^4.6.0",
		"@devsnicket/eunice-renderer-test-harness": "^1.7.0",
		"js-yaml": "3.13.1",
		"minimist": "1.2.0"
	},
	"devDependencies": {
		"@babel/core": "7.4.5",
		"@babel/polyfill": "7.4.4",
		"@babel/preset-env": "7.4.5",
		"@devsnicket/eunice-call-with-process-standard-streams": "*",
		"@devsnicket/eunice-dependency-and-structure": "*",
		"@devsnicket/eunice-run-tests-from-file-system": "*",
		"@devsnicket/eunice-test-harnesses": ">=7.4.0",
		"@devsnicket/eunice-test-harnesses-processor-plugins": "*",
		"@devsnicket/plugin-discovery-commonjs-babel-plugin": "1.11.1",
		"@types/node": "12.0.2",
		"babel-loader": "8.0.6",
		"cspell": "4.0.7",
		"eslint": "5.16.0",
		"webpack-cli": "3.3.2"
	},
	"homepage": "https://github.com/DevSnicket/eunice-javascript#readme",
	"keywords": [
		"analyze",
		"analyzer",
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"javascript",
		"render",
		"stack",
		"structure",
		"svg",
		"uince",
		"unice",
		"yaml"
	],
	"license": "MIT",
	"main": "index.js",
	"name": "@devsnicket/eunice",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-javascript.git"
	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run dogfooding && npm run harness",
		"dogfooding": "npm update --no-save && node bin --directoryToCreateOrAddToStacksFrom=dogfooding --outputBaseFileName= --outputDirectoryPath=dogfooding --packageNames=call-with-process-standard-streams --packageNames=dependency-and-structure --packageNames=javascript-analyzer --packageNames=processors --packageNames=renderer --packageNames=renderer-test-harness --packageNames=run-tests-from-file-system --packageNames=test-harnesses --packageNames=test-harnesses-processor-plugins --packagePrefix=eunice- --packageScope=devsnicket --directories=. --rootItemIdentifiers=javascript",
		"lint": "npx eslint .",
		"harness": "(cd harness && rm -rf output && mkdir output && npx webpack -p --config webpack.config.js)",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./harness/output/**\" ! -path \"./node_modules/**/*\" ! -path \"./packageWithoutScope/node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +"
	},
	"version": "4.0.0"
}
