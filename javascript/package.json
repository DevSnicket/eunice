{
	"author": "Graham Dyson",
	"bin": {
		"eunice": "dist/bin.js"
	},
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Eunice is based on the premise that software structure and modularity improve when dependencies are unidirectional. Eunice analyses source code, infers its structure and shows if the dependencies match.",
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"@babel/node": "^7.10.5",
		"@babel/plugin-transform-modules-commonjs": "^7.10.4",
		"@babel/polyfill": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/register": "^7.10.5",
		"@devsnicket/eunice-call-with-process-standard-streams": "^1.8.0",
		"@devsnicket/eunice-dependency-and-structure": "^6.0.0",
		"@devsnicket/eunice-interactive": "^5.10.0",
		"@devsnicket/eunice-javascript-analyzer": "^11.33.0",
		"@devsnicket/eunice-replacement": "^14.5.0",
		"@devsnicket/eunice-run-tests-from-file-system": "^4.6.1",
		"@devsnicket/eunice-stacking-explicit": "^14.6.0",
		"@devsnicket/eunice-stacking-inference": "^4.0.0",
		"@devsnicket/eunice-test-harnesses": "^9.1.0",
		"@devsnicket/eunice-visualizer": "^2.1.0",
		"@types/fs-extra": "^8.1.1",
		"@types/jest": "^26.0.7",
		"@types/lodash.deburr": "^4.1.6",
		"@types/node": "^12.12.53",
		"@types/react": "^16.9.43",
		"@types/react-dom": "^16.9.8",
		"@types/string-pixel-width": "^1.7.1",
		"@types/webpack": "^4.41.21",
		"babel-loader": "^8.1.0",
		"core-js": "^3.6.5",
		"cspell": "^4.0.63",
		"eslint": "^7.5.0",
		"eslint-plugin-notice": "^0.9.10",
		"fs-extra": "^8.1.0",
		"jest": "^26.1.0",
		"js-yaml": "^3.14.0",
		"lodash": "^4.17.19",
		"minimist": "^1.2.5",
		"string-pixel-width": "^1.10.0",
		"supports-color": "^7.1.0",
		"terser": "^4.8.0",
		"typescript": "^3.9.7",
		"webpack-cli": "^3.3.12"
	},
	"files": [
		"dist/**/*",
		"third-party-notices.txt"
	],
	"homepage": "https://devsnicket.com/eunice",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!analyzeAndProcessAndRender/analyzeAndProcess/tests/sources/**",
			"!analyzeAndProcessAndRender/tests/outputEnabledForHtmlAndSvg/source/**",
			"!analyzeAndProcessAndRender/tests/packageSources/node_modules/**",
			"!analyzeAndProcessAndRender/tests/singleSource/source/**",
			"!analyzeAndProcessAndRender/tests/singleSourceWithOutputBaseFilename/source/**",
			"!webpack.config.js",
			"!dist/**",
			"!dogfooding/output/**",
			"!gh-pages/**",
			"!harness/**",
			"!node_modules/**",
			"!harness/output/**",
			"!interactive.webpack.config.js",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		},
		"transformIgnorePatterns": [
			"node_modules/(?!@devsnicket)"
		]
	},
	"keywords": [
		"analyze",
		"analyzer",
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"javascript",
		"render",
		"stack",
		"structure",
		"svg",
		"uince",
		"unice",
		"yaml"
	],
	"license": "SEE LICENSE IN LICENSE",
	"name": "eunice",
	"publishConfig": {
		"repository": "https://pkgs.dev.azure.com/grahamdyson/_packaging/Eunice/npm/registry/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/devsnicket/eunice.git"
	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run typescript && npm run build-prepare && npm run build-interactive && npm run test && npm run build && npm run build-remove-maps && npm run harness && npm run dogfooding",
		"build": "npx webpack -p --config webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs",
		"build-remove-maps": "rm dist/bin.js.map && rm dist/interactive/harness.js.map",
		"build-development": "npx webpack -d --config webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs",
		"build-interactive": "npx webpack -p --config interactive.webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs",
		"build-interactive-development": "npx webpack -d --config interactive.webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs",
		"build-prepare": "rm -rf dist && mkdir dist",
		"dogfooding": "(cd dogfooding && rm -rf output && mkdir output && cd .. && node dist/bin.js --accept-license --directory-to-create-or-add-to-stacks-from=dogfooding --include-service-workers=true --ignore-path-pattern=\"(^|/)(\\..*|dist|gh-pages|node_modules|output|test-cases|test-coverage)$\" --modify-stacks-key=type --modify-stacks-pattern=^file$ --output-base-file-name=index --output-directory-path=dogfooding/output --output-svg --output-yaml --package-names=call-with-process-standard-streams --package-names=dependency-and-structure --package-names=dependency-counter --package-names=interactive --package-names=javascript-analyzer --package-names=replacement --package-names=run-tests-from-file-system --package-names=stacking-explicit --package-names=stacking-inference --package-names=test-harnesses --package-names=visualizer --package-prefix=eunice- --package-scope=devsnicket --directories=. --reverseFileContent=false --root-item-identifiers=javascript)",
		"lint": "npx eslint .",
		"harness": "(rm -rf harness/output && npx webpack -p --config harness/webpack.config.js -r @devsnicket/eunice-test-harnesses/babel.register.cjs)",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./dogfooding/output/**\" ! -path \"./dist/*\" ! -path \"./gh-pages/**\" ! -path \"./harness/output/**\" ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +",
		"test": "jest",
		"typescript": "npx tsc -p jsconfig.json"
	},
	"version": "7.13.0"
}
