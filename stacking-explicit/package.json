{
	"author": "Graham Dyson",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Processes YAML files of structure and dependencies, generated by analyzers, so they can be rendered and interacted with.",
	"dependencies": {
		"core-js": "3.6.4",
		"js-yaml": "3.13.1"
	},
	"devDependencies": {
		"@babel/core": "7.8.4",
		"@babel/plugin-transform-modules-commonjs": "7.8.3",
		"@types/jest": "24.0.19",
		"@types/node": "10.12.12",
		"cspell": "4.0.21",
		"eslint": "7.3.1",
		"eslint-plugin-notice": "0.9.10",
		"jest": "24.9.0",
		"typescript": "3.7.5"
	},
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!**/bin.js",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		},
		"transformIgnorePatterns": [
			"node_modules/(?!@devsnicket)"
		]
	},
	"keywords": [
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"process",
		"processors",
		"stack",
		"structure",
		"uince",
		"unice",
		"yaml"
	],
	"license": "MIT",
	"name": "@devsnicket/eunice-processors",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-processors.git"
	},
	"sideEffects": false,
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run typescript && npm run test",
		"eunice": "npx eunice --accept-license --ignore-path-pattern=\"(^|/)(\\..*|node_modules|test-coverage)$\" --output-yaml --reverse-file-content=false",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +",
		"test": "npx jest",
		"typescript": "npx tsc -p jsconfig.json"
	},
	"version": "14.4.0"
}
