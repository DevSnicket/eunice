{
	"author": "Graham Dyson",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Takes YAML objects, replaces the identifiers of whats depended upon with references, adds dependent references in other direction and normalises the hierarchical structure.",
	"dependencies": {
		"array.prototype.flat": "1.2.1",
		"array.prototype.flatmap": "1.2.1",
		"pretty-format": "23.5.0"
	},
	"devDependencies": {
		"@types/jest": "24.0.18",
		"@types/node": "10.12.18",
		"cspell": "4.0.21",
		"eslint": "6.2.1",
		"eslint-plugin-notice": "0.8.9",
		"jest": "24.9.0"
	},
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!index.js",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"keywords": [
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"stack",
		"structure",
		"uince",
		"unice",
		"yaml"
	],
	"license": "MIT",
	"main": "index.js",
	"name": "@devsnicket/eunice-dependency-and-structure",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-dependency-and-structure.git"
	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run test",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +",
		"test": "npx jest --runInBand"
	},
	"version": "2.13.0"
}
