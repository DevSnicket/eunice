{
	"author": "Graham Dyson",
	"description": "Takes YAML objects, replaces identifiers of whats depended upon with references, adds dependent references and normalises structure.",
	"dependencies": {
		"@types/jest": "23.3.1",
		"array.prototype.flat": "1.2.1",
		"array.prototype.flatmap": "1.2.1",
		"babel-preset-env": "1.7.0",
		"jest": "23.3.0",
		"pretty-format": "23.5.0"
	},
	"devDependencies": {
		"eslint": "4.19.1"
	},
	"license": "MIT",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"scripts": {
		"lint": "npx eslint .",
		"test": "npx jest --runInBand"
	}
}
