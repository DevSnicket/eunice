{
	"author": "Graham Dyson",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Takes YAML objects, replaces the identifiers of whats depended upon with references, adds dependent references in other direction and normalises the hierarchical structure.",
	"dependencies": {
		"core-js": "3.6.4"
	},
	"devDependencies": {
		"@babel/core": "7.10.4",
		"@babel/plugin-transform-modules-commonjs": "7.10.4",
		"@types/jest": "26.0.4",
		"@types/node": "14.0.18",
		"cspell": "4.0.63",
		"eslint": "7.4.0",
		"eslint-plugin-notice": "0.9.10",
		"jest": "26.1.0",
		"pretty-format": "26.1.0",
		"typescript": "3.9.6"
	},
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!index.js",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		},
		"transformIgnorePatterns": [
			"node_modules/(?!@devsnicket)"
		]
	},
	"keywords": [
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"stack",
		"structure",
		"uince",
		"unice",
		"yaml"
	],
	"license": "SEE LICENSE IN LICENSE",
	"main": "index.js",
	"name": "@devsnicket/eunice-dependency-and-structure",
	"repository": {
		"type": "git",
		"url": "https://github.com/grahandyson/eunice-dependency-and-structure.git"
	},
	"sideEffects": false,
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run typescript && npm run test",
		"eunice": "npx eunice --accept-license --ignore-path-pattern=\"(^|/)(\\..*|node_modules|output|test-coverage)$\" --output-yaml --reverse-file-content=false",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec cspell {} +",
		"test": "npx jest",
		"typescript": "npx tsc -p jsconfig.json"
	},
	"version": "4.7.0"
}
