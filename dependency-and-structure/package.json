{
	"author": "Graham Dyson",
	"babel": {
		"plugins": [
			"@babel/plugin-transform-modules-commonjs"
		]
	},
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Takes YAML objects, replaces the identifiers of whats depended upon with references, adds dependent references in other direction and normalises the hierarchical structure.",
	"dependencies": {
		"core-js": "3.6.4"
	},
	"devDependencies": {
		"@babel/plugin-transform-modules-commonjs": "7.8.3",
		"@types/jest": "24.0.18",
		"@types/node": "10.12.18",
		"cspell": "4.0.21",
		"eslint": "6.8.0",
		"eslint-plugin-notice": "0.8.9",
		"jest": "24.9.0",
		"pretty-format": "25.1.0",
		"typescript": "3.7.5"
	},
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!index.js",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"keywords": [
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"stack",
		"structure",
		"uince",
		"unice",
		"yaml"
	],
	"license": "SEE LICENSE IN LICENSE",
	"main": "index.js",
	"name": "@devsnicket/eunice-dependency-and-structure",
	"repository": {
		"type": "git",
		"url": "https://github.com/grahandyson/eunice-dependency-and-structure.git"
	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run test && npm run typescript",
		"eunice": "npx eunice --accept-license --ignore-path-pattern=\"(^|/)(\\..*|node_modules|output|test-coverage)$\" --output-yaml --reverse-file-content=false",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec cspell {} +",
		"test": "npx jest --runInBand",
		"typescript": "npx tsc -p jsconfig.json"
	},
	"version": "4.3.0"
}
