{
	"author": "Graham Dyson",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Takes YAML objects, replaces the identifiers of whats depended upon with references, adds dependent references in other direction and normalises the hierarchical structure.",
	"dependencies": {
		"array.prototype.flat": "1.2.1",
		"array.prototype.flatmap": "1.2.1",
		"pretty-format": "23.5.0"
	},
	"devDependencies": {
		"@types/jest": "23.3.1",
		"@types/node": "10.12.18",
		"babel-cli": "6.26.0",
		"babel-plugin-transform-object-rest-spread": "6.26.0",
		"babel-preset-env": "1.7.0",
		"cspell": "3.1.3",
		"eslint": "4.19.1",
		"jest": "23.3.0"
	},
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!dist/**",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"keywords": [
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"stack",
		"structure",
		"uince",
		"unice",
		"yaml"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"name": "@devsnicket/eunice-dependency-and-structure",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-dependency-and-structure.git"
	},
	"scripts": {
		"build": "npx babel -d dist --ignore dist/*,dist/**/*,node_modules/**/*,**/tests/**,**/tests/**,test-coverage/**,**/test.js --plugins transform-object-rest-spread -s true .",
		"lint": "npx eslint .",
		"spellcheck": "npx cspell *.js **/*.js *.md **/*.md",
		"test": "npx jest --runInBand"
	},
	"version": "2.3.0"
}
