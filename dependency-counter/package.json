{
	"author": "Graham Dyson",
	"bin": "dist/getSvgForYaml/bin.js",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Visualize, explore and statistically measure how actual dependencies match intendend structure in YAML files generated by analyzers.",
	"dependencies": {
		"@devsnicket/eunice-call-with-process-standard-streams": "1.0.0",
		"@devsnicket/eunice-dependency-and-structure": "1.1.2",
		"array.prototype.flatmap": "1.2.1",
		"babel-polyfill": "6.26.0",
		"js-yaml": "3.12.0",
		"react": "16.4.0",
		"react-dom": "16.4.0",
		"string-pixel-width": "1.7.0"
	},
	"devDependencies": {
		"@devsnicket/eunice-run-tests-from-file-system": "1.4.0",
		"@types/jest": "23.3.10",
		"@types/js-yaml": "3.11.2",
		"@types/react": "16.4.18",
		"@types/react-dom": "16.0.11",
		"babel-cli": "6.26.0",
		"babel-plugin-transform-object-rest-spread": "6.26.0",
		"babel-preset-env": "1.7.0",
		"eslint": "5.9.0",
		"jest": "23.6.0",
		"react-reflex": "2.2.8"
	},
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!bin/**",
			"!dist/**",
			"!node_modules/**",
			"!harness/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"keywords": [
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"render",
		"stack",
		"structure",
		"svg",
		"uince",
		"unice",
		"yaml"
	],
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"license": "MIT",
	"name": "@devsnicket/eunice-renderer",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-renderer.git"
	},
	"scripts": {
		"build": "npx babel -d dist --ignore dist/*,dist/**/*,node_modules/**/*,**/test/**,test-coverage/**,**/test.js --plugins transform-object-rest-spread -s true .",
		"lint": "npx eslint .",
		"test": "npx jest --runInBand"
	},
	"version": "1.3.0"
}
