{
	"author": "Graham Dyson",
	"bin": "getSvgForYaml/bin.js",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Visualize, explore and statistically measure how actual dependencies match intended structure in YAML files generated by analyzers.",
	"dependencies": {
		"@devsnicket/eunice-call-with-process-standard-streams": "^1.3.0",
		"@devsnicket/eunice-dependency-and-structure": "^2.13.0",
		"array.prototype.flatmap": "1.2.1",
		"js-yaml": "3.13.1",
		"react": "16.9.0",
		"react-dom": "16.9.0",
		"string-pixel-width": "1.7.0"
	},
	"devDependencies": {
		"@devsnicket/eunice-run-tests-from-file-system": "2.1.0",
		"@types/jest": "24.0.18",
		"@types/js-yaml": "3.12.1",
		"@types/lodash.deburr": "4.1.6",
		"@types/node": "11.13.8",
		"@types/react": "16.9.2",
		"@types/react-dom": "16.9.0",
		"@types/string-pixel-width": "1.7.0",
		"cspell": "4.0.21",
		"eslint": "6.3.0",
		"eslint-plugin-notice": "0.8.9",
		"jest": "24.9.0",
		"typescript": "3.4.5"
	},
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!**/bin.js",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"keywords": [
		"architecture",
		"dependency",
		"dependencies",
		"euince",
		"render",
		"stack",
		"structure",
		"svg",
		"uince",
		"unice",
		"yaml"
	],
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"license": "SEE LICENSE IN LICENSE",
	"main": "index.js",
	"name": "@devsnicket/eunice-renderer",
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-renderer.git"
	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run typescript && npm run test",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +",
		"test": "npx jest --runInBand",
		"typescript": "npx tsc -p jsconfig.json"
	},
	"version": "6.2.0"
}
