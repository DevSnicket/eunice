{
	"author": "Graham Dyson",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "counts dependencies into categories and aggregates the counts of descendants",
	"dependencies": {
		"@devsnicket/eunice-dependency-and-structure": "^4.10.1",
		"core-js": "^3.6.5"
	},
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"@babel/node": "^7.10.5",
		"@babel/plugin-transform-modules-commonjs": "^7.10.4",
		"@devsnicket/eunice-run-tests-from-file-system": "^4.6.1",
		"@types/fs-extra": "^8.1.1",
		"@types/jest": "^26.0.5",
		"@types/js-yaml": "^3.12.5",
		"@types/lodash.deburr": "^4.1.6",
		"@types/node": "^14.0.24",
		"cspell": "^4.0.63",
		"eslint": "^7.5.0",
		"eslint-plugin-notice": "^0.9.10",
		"jest": "^26.1.0",
		"js-yaml": "^3.14.0",
		"typescript": "^3.9.7"
	},
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!**/bin.js",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		},
		"transformIgnorePatterns": [
			"node_modules/(?!@devsnicket)"
		]
	},
	"homepage": "https://devsnicket.com/eunice",
	"license": "SEE LICENSE IN LICENSE",
	"main": "index.js",
	"name": "@devsnicket/eunice-dependency-counter",
	"publishConfig": {
		"repository": "https://pkgs.dev.azure.com/grahamdyson/_packaging/Eunice/npm/registry/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-dependency-counter.git"
	},
	"sideEffects": false,
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run typescript && npm run test",
		"eunice": "npx -p eunice eunice --accept-license --ignore-path-pattern=\\\"(^|/)(\\..*|node_modules|test-cases|test-coverage)$\\\" --output-yaml --reverse-file-content=false",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +",
		"test": "npx jest",
		"typescript": "npx tsc -p jsconfig.json"
	},
	"version": "2.0.0"
}
