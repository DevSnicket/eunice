{
	"author": "Graham Dyson",
	"bugs": {
		"url": "https://github.com/DevSnicket/eunice/issues"
	},
	"description": "Runs tests by discovering test cases and expected behaviour from the file system. Tests are automatically named based on the directory hierarchy.",
	"devDependencies": {
		"@types/jest": "24.0.15",
		"cspell": "4.0.21",
		"eslint": "6.0.1",
		"eslint-plugin-notice": "0.8.8",
		"fs-extra": "8.1.0",
		"jest": "24.8.0"
	},
	"homepage": "https://github.com/DevSnicket/Eunice#readme",
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!node_modules/**",
			"!test-coverage/**"
		],
		"coverageDirectory": "test-coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"keywords": [
		"automatic",
		"directory",
		"directories",
		"discovery",
		"euince",
		"file",
		"files",
		"folder",
		"folders",
		"hierarchy",
		"hierarchical",
		"jest",
		"naming",
		"test",
		"testing",
		"uince",
		"unice"
	],
	"license": "MIT",
	"main": "index.js",
	"name": "@devsnicket/eunice-run-tests-from-file-system",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DevSnicket/eunice-run-tests-from-file-system.git"
	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run test",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./dist/*\" ! -path \"./dist/**/*\" ! -path \"./node_modules/**/*\" ! -path \"./test-coverage/**/*\" -exec npx cspell {} +",
		"test": "npx jest --runInBand"
	},
	"version": "1.7.0"
}
